{
  "name": "pdf-annotate-web",
  "version": "0.0.0",
  "description": "PDF Annotation Tool",
  "scripts": {
    "link:deps": "echo 'No need to link dependencies'",
    "dev": "cross-env VITE_API_ENV=dev bun run --bun vite --host",
    "dev:uat": "cross-env VITE_API_ENV=uat bun run --bun vite",
    "dev:prod": "cross-env VITE_API_ENV=prod bun run --bun vite",
    "check": "bun run link:deps && node ./node_modules/vue-tsc/bin/vue-tsc --noEmit",
    "build:_": "cross-env NODE_ENV=production SKIP_TYPECHECK=true bun run --bun vite build",
    "build": "cross-env bun run build:_",
    "build:dev": "cross-env VITE_API_ENV=dev bun run build:_",
    "build:uat": "cross-env VITE_API_ENV=uat bun run build:_",
    "postinstall": "patch-package --error-on-fail && echo 'No need to link dependencies'",
    "preview": "bun run --bun vite preview",
    "cdn": "bun run node scripts/cdn.js",
    "lang": "bun run ts-node scripts/lang.ts",
    "test": "bun run ts-node ./build/plugins/postcss-plugin-theme-color/test.ts",
    "build:core": "bun run tsc --project ./src/pdf-annotate-core/tsconfig.json",
    "build:viewer": "bun run tsc --project ./src/pdf-annotate-viewer/tsconfig.json",
    "build:all": "bun run build && bun run build:core && bun run build:viewer",
    "lint": "eslint --ext .js,.vue,.ts src",
    "lint:fix": "eslint --ext .js,.vue,.ts src --fix",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:dev:local": "cross-env API_TARGET=http://localhost:8081 tauri dev",
    "tauri:build": "tauri build",
    "tauri:build:full": "bash scripts/build-sidecar.sh && tauri build",
    "tauri:sidecar": "bash scripts/build-sidecar.sh",
    "tauri:icon": "tauri icon"
  },
